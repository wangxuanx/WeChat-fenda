.cu-card>.cu-item {
  margin-left: 0rpx;
  margin-right: 0rpx; 
  margin-bottom: 0;
  margin-top: 0rpx;
  border-bottom: 1rpx solid #ddd;
}

.addAction {
  position: fixed;
  right: 3%;
  top: 85%;
  opacity: 1;
  z-index: 555;
  border-radius: 120px;
  background-color: #fff;
  height: 50px;
  width: 50px;
  box-shadow: 0px 0px 5px #ccc;
}

